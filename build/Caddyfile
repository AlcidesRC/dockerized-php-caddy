{
	supervisor {
		php-fpm --nodaemonize {
			env DEBUG false

			redirect_stdout stdout
			redirect_stderr stdout

			restart_policy on_failure
		}
	}
}

website.localhost {
    tls internal

    respond /healthcheck 200

    root * /var/www/html/public

    encode zstd gzip

    php_fastcgi :9000

    file_server
}
